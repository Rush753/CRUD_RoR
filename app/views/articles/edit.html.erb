<h1>Edit article</h1>

<%= form_with(model: @article) do |form| %>

  <p>
    <%= form.label :id %><br>
    <%= form.text_field :id %>
  </p>

  <p>
    <%= form.label :fname %><br>
    <%= form.text_field :fname %>
  </p>

  <p>
    <%= form.label :lname %><br>
    <%= form.text_field :lname %>
  </p>

  <p>
    <%= form.label :age %><br>
    <%= form.text_field :age %>
  </p>

  <p>
    <%= form.submit %>
  </p>

<% end %>

<%= link_to 'Back', articles_path %>